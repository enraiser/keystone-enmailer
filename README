var enmailer('keystone-enmailer');

var Email = new keystone.Email('welcome');
Email.send({
    mandrill: enmailer.mandrill,
    comments: "the comments",
    subject: 'the subject',
    to: "someone@somewhere.com",
    from: {
        name: 'Sachin Sharma',
        email: 'admin@lastwish.me'
    }
});
